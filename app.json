{
  "expo": {
    "name": "MDapp",
    "slug": "MDapp",
    "version": "1.0.0",
    "description": "Social app for MD Dance School",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F8BE50"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "hu.franciskav.md_app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
